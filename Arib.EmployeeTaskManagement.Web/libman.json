{
  "version": "3.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "datatable@2.0.2",
      "destination": "wwwroot/lib/datatable/",
      "files": [
        "js/datatable.min.js",
        "css/datatable.min.css",
        "img/sort_asc.png",
        "img/sort_both.png",
        "img/sort_desc.png"
      ]
    },
    {
      "library": "sweetalert2@11.22.0",
      "destination": "wwwroot/lib/sweetalert2/"
    },
    {
      "library": "datatables.net-bs5@2.3.2",
      "destination": "wwwroot/lib/datatables.net-bs5/"
    },
    {
      "library": "fontawesome-iconpicker@3.2.0",
      "destination": "wwwroot/lib/fontawesome-iconpicker/",
      "files": [
        "js/fontawesome-iconpicker.min.js",
        "css/fontawesome-iconpicker.min.css"
      ]
    },
    {
      "library": "bootstrap-icons@1.13.1",
      "destination": "wwwroot/lib/bootstrap-icons/",
      "files": [
        "bootstrap-icons.svg",
        "font/fonts/bootstrap-icons.woff",
        "font/fonts/bootstrap-icons.woff2",
        "font/bootstrap-icons.css",
        "font/bootstrap-icons.json",
        "font/bootstrap-icons.min.css"
      ]
    }
  ]
}